function getScript(url,callback,charset,param){var head=document.getElementsByTagName('HEAD')[0];var script=document.createElement('script');script.type='text/javascript';if(charset!=null){script.charset="UTF-8";}
var loaded=false;script.onreadystatechange=function(){if(this.readyState=='loaded'||this.readyState=='complete'){if(loaded){return;}
loaded=true;if(param!=null){callback(param);}else{try{callback();}catch(e){}}}}
script.onload=function(){if(loaded){return;}
loaded=true;if(param!=null){callback(param);}else{callback();}}
script.src=url;head.appendChild(script);}
