(function(){var ua=navigator.userAgent;var browsers={'InternetExplorer':{nameExp:/(?:MSIE)+|(?:Trident)+/,verExp:/(?:MSIE) ([0-9.]+)/},'Firefox':{nameExp:/(?:Firefox)+/,verExp:/(?:Firefox)\/([0-9.]+)/},'Chrome':{nameExp:/(?:Chrome)+/,verExp:/(?:Chrome)\/([0-9.]+)/},'Opera':{nameExp:/(?:Opera)+/,verExp:/(?:Version)\/([0-9.]+)/},'Safari':{nameExp:/(?:AppleWebKit)+/,verExp:/(?:Version)\/([0-9.]+)/}};var OSs={'Windows':{nameExp:/(?:Win)+/,verExp:/(?:Windows NT) ([0-9.]+)/},'Mac':{nameExp:/(?:Mac)+/,verExp:null},'Linux':{nameExp:/(?:Linux)+|(?:X11)+/,verExp:null}};var bd=window.browserDetector={getBrowserInfo:function(iWannaRealVersion,stringLikeUserAgent){var us=(stringLikeUserAgent)?stringLikeUserAgent:ua,rtnObj={};for(var key in browsers){if(browsers[key].nameExp.test(us)){switch(key){case'Safari':if(us.indexOf('Chrome')==-1){rtnObj.name=key;rtnObj.ver=parseInt(us.match(browsers[key].verExp)[1],10)}break;case'InternetExplorer':rtnObj.name=key;var verResult=us.match(browsers[key].verExp);if(verResult){rtnObj.ver=parseInt(verResult[1],10)}else{rtnObj.ver=parseInt(us.match(/(?:rv)\:([0-9.]+)/)[1],10)}rtnObj.compatibleMode=false;if(rtnObj.ver==7){if(us.indexOf('Trident')>=0){rtnObj.compatibleMode=true}}if(iWannaRealVersion){if(rtnObj.ver<11){rtnObj.realVer=this.getRealInternetExploreVersion()}else{rtnObj.realVer='11(+)'}}rtnObj.activexSupport=this.getActiveXSupport();break;default:rtnObj.name=key;rtnObj.ver=parseInt(us.match(browsers[key].verExp)[1],10);break}}}return rtnObj},getRealInternetExploreVersion:function(){var obj=document.createElement('div'),CLASSID=['{45EA75A0-A269-11D1-B5BF-0000F8051515}','{3AF36230-A269-11D1-B5BF-0000F8051515}','{89820200-ECBD-11CF-8B85-00AA005B4383}'],realVersion=null;try{obj.style.behavior='url(#default#clientcaps)'}catch(ex){}for(var i=0;i<CLASSID.length;i++){try{realVersion=obj.getComponentVersion(CLASSID[i],'componentid').replace(/,/g,'.')}catch(ex){}if(realVersion){break}}return realVersion},getActiveXSupport:function(){var rtn;try{rtn=!!new ActiveXObject('htmlfile')}catch(ex){rtn=false}return rtn},getOSInfo:function(stringLikeUserAgent){var us=(stringLikeUserAgent)?stringLikeUserAgent:ua,rtnObj={};for(var key in OSs){if(OSs[key].nameExp.test(us)){switch(key){case'Windows':rtnObj.name='Windows';var ver=us.match(OSs[key].verExp)[1];switch(ver){case'5.0':rtnObj.ver='Windows 2000';break;case'5.1':rtnObj.ver='Windows XP';break;case'5.2':rtnObj.ver='Windows Server 2003';break;case'6.0':rtnObj.ver='Windows Vista';break;case'6.1':rtnObj.ver='Windows 7';break;case'6.2':rtnObj.ver='Windows 8';break;default:rtnObj.ver='before Windows 2000 or after Windows 8';break}break;default:rtnObj.name=key;break}}}return rtnObj}}})();