var mainad          = new Cyad('mainBasic');
mainad.BinaryType   = 'image';
mainad.IntroSrc     = 'https://adimg.nate.com/img/2019/08/meritz/meritz_0826_595x100.jpg';
mainad.ClickUrl     = 'http://cyad1.nate.com/click.kti/nate/main3@m_Top1?ads_no=191093&cmp_no=26261&img_no=304552';
mainad.StatScriptUrl1 = '';
mainad.StatStr      = 'ad.nate.com/nate/main/top/';
mainad.RefStr       = 'ad01';

//mainad setting 2018-05-30 02:21
try{if(!CyadLib)var CyadLib={};CyadLib.hasOwnProperty("prefixProtocol")||(CyadLib.prefixProtocol=function(r){function t(r){var t=location.protocol;return/^http[s]?:/.test(t)&&(r=r.replace(/http:/g,t).replace(/https:/g,t)),/^\/\/\w+?/.test(r)&&(r=t+r),/^http[s]?\/\//.test(r)&&(r=r.replace(/http\/\//g,t+"//").replace(/https\/\//g,t+"//")),r}r.IntroSrc&&(r.IntroSrc=t(r.IntroSrc)),r.CoverSrc&&(r.CoverSrc=t(r.CoverSrc)),r.ExtendedSrc&&(r.ExtendedSrc=t(r.ExtendedSrc)),r.ClickUrl&&(r.ClickUrl=t(r.ClickUrl)),r.ClickUrl1&&(r.ClickUrl1=t(r.ClickUrl1)),r.ClickUrl2&&(r.ClickUrl2=t(r.ClickUrl2)),r.ClickUrl3&&(r.ClickUrl3=t(r.ClickUrl3)),r.StatScriptUrl1&&(r.StatScriptUrl1=t(r.StatScriptUrl1)),r.StatScriptUrl2&&(r.StatScriptUrl2=t(r.StatScriptUrl2)),r.StatScriptUrl3&&(r.StatScriptUrl3=t(r.StatScriptUrl3)),r.StatScriptUrl4&&(r.StatScriptUrl4=t(r.StatScriptUrl4)),r.FlvSrc&&(r.FlvSrc=t(r.FlvSrc)),r.ContainSrc&&(r.ContainSrc=t(r.ContainSrc)),r.ContainSrc2&&(r.ContainSrc2=t(r.ContainSrc2)),r.StaticVersionSrc&&(r.StaticVersionSrc=t(r.StaticVersionSrc)),r.StaticVersionSrc2&&(r.StaticVersionSrc2=t(r.StaticVersionSrc2)),r.VideoUrl&&(r.VideoUrl=t(r.VideoUrl)),r.CountUrl&&(r.CountUrl=t(r.CountUrl))}),CyadLib.prefixProtocol(mainad)}catch(r){}