function leftByteStr(strObj,lenByte,overtext){var str=strObj;var intLen=0;var intSize=0;var boolLen=false;for(j=0;j<str.length;j++){var chr=str.charAt(j);if(chr.charCodeAt()>128){intLen+=1.4;}
else{if(65<=chr.charCodeAt()&&chr.charCodeAt()<=90){intLen++;}
else if(97<=chr.charCodeAt()&&chr.charCodeAt()<=122){intLen+=0.75;}
else if(32<=chr.charCodeAt()&&chr.charCodeAt()<=64){intLen+=0.6;}
else if(91<=chr.charCodeAt()&&chr.charCodeAt()<=96){intLen+=0.6;}
else if(123<=chr.charCodeAt()&&chr.charCodeAt()<=126){intLen+=0.6;}}
if(intLen<=lenByte){intSize++;}
else{boolLen=true;}}
var word=str.substring(0,intSize)
if(boolLen&&overtext!="none"){word=word+"...";}
return word;}
