function base64encode(s){var t='',p=-6,a=0,i=0,v=0,c;while(i<s.length||p>-6){if(p<0){if(i<s.length){c=s.charCodeAt(i++);v+=8;}else{c=0;}
a=((a&255)<<8)|(c&255);p+=8;}
if(v>0){t+=base64list.charAt((a>>p)&63);}else{t+=base64list.charAt(64);}
p-=6;v-=6;}
return t;}
