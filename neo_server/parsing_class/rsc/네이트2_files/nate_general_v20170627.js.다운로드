function base64decode(s){var t='',p=-8,a=0,q=0,c,m,n;for(i=0;i<s.length;i++){c=base64list.indexOf(s.charAt(i));if(c<0)continue;a=(a<<6)|(c&63);p+=6;if(p>=0){c=(a>>p)&255;if(c>0)t+=String.fromCharCode(c);a&=63;p-=8;}}return t;}
