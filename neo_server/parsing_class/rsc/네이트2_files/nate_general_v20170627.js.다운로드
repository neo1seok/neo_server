Newsbox.prototype.callBackloadJSFile=function(fileCode){switch(fileCode){case'tab1_set1':rTab1Set1Img=rTab1Set1Data['rTab1Set1Img'];rTab1Set1Txt=rTab1Set1Data['rTab1Set1Txt'];rTab1Set1Plus=rTab1Set1Data['rTab1Set1Plus'];break;case'tab1_set2':rTab1Set2Img=rTab1Set2Data['rTab1Set2Img'];rTab1Set2Txt=rTab1Set2Data['rTab1Set2Txt'];rTab1Set2Plus=rTab1Set2Data['rTab1Set2Plus'];break;case'tab1_set3':rTab1Set3Img=rTab1Set3Data['rTab1Set3Img'];rTab1Set3Txt=rTab1Set3Data['rTab1Set3Txt'];rTab1Set3Plus=rTab1Set3Data['rTab1Set3Plus'];break;case'tab2_spo':rTab2SpoImg=rTab2SpoData['rEntSpoSet1Img'];rTab2SpoTxt=rTab2SpoData['rEntSpoSet1Txt'];break;case'tab3_ent':rTab3EntImg=rTab3EntData['rEntSpoSet2Img'];rTab3EntTxt=rTab3EntData['rEntSpoSet2Txt'];break;case'tab4_special':rTab4SpeImg=rTab4SpeData['rTab4SpeImg'];rTab4SpeTxt=rTab4SpeData['rTab4SpeTxt'];break;case'tab5_ranking':break;}
rNewsJsLoaded[fileCode]=true;setTimeout("newsBox.loadJSContents('"+fileCode+"');",200);newsBox.rNowRolling=newsBox.setNewsRolling(fileCode);newsBox.rNowRolling.push(newsBox.rNowRolling.shift());clearTimeout(objNewsTimer);objNewsTimer=setTimeout("newsBox.loadContents('"+newsBox.rNowRolling[0]+"');",newsBox.getRollingTime());}
Newsbox.prototype.loadJSContents=function(fileCode){var bChk=this.showContents(fileCode);if(bChk==false){this.showContents(this.sNowFileCode);}else{rNewsJsLoaded[fileCode]=true;this.sNowFileCode=fileCode;}}
Newsbox.prototype.loadContents=function(fileCode,objJSFile){if(!newsBox){return;}
