function checkParent(target,element){var parentNode;if(target===null)return false;while(parentNode=target.parentNode||target.offsetParent){if(target==element)
return true;target=parentNode;}
return false;}
function addMouseEnterEventListener(element,fn){var callback=function(e){var e=e||window.event,target=e.target||e.srcElement,relatedTarget=e.relatedTarget||e.fromElement;if(checkParent(relatedTarget,element))
return;if(fn)fn();};addEvent(element,'mouseover',callback);}
function addMouseLeaveEventListener(element,fn){var callback=function(e){var e=e||window.event,target=e.target||e.srcElement,relatedTarget=e.relatedTarget||e.toElement;if(checkParent(relatedTarget,element))
return;if(fn)fn();};addEvent(element,'mouseout',callback);}
