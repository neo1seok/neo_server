function getMyHomeType(tid,returnType){if(tid==""){return;}
if(tid==undefined){return;}
if(returnType==undefined){returnType="C";}
tid=tid.toString();var firstLetter=tid.substring(0,1).toUpperCase();var myHomeType="";if(firstLetter=="C"&&tid.length==9){myHomeType=(returnType=="C")?"C":"ⓒ로그";}
else if(IsValidC2ID(tid)){myHomeType=(returnType=="C")?"B":"블로그";}
else if(firstLetter=='7'&&tid.length==8){myHomeType=(returnType=="C")?"T":"타운홈피";}
else{myHomeType=(returnType=="C")?"M":"미니홈피";}
return myHomeType;}
